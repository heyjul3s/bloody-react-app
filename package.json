{
  "name": "bloody-react-app",
  "version": "1.0.2",
  "author": "Julian Low <julian.cy.low@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heyjul3s/bloody-react-app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heyjul3s/bloody-react-app"
  },
  "description": "A basic Typescript React app boilerplate to help you get started sans middleware store.",
  "main": "./src/index.js",
  "keywords": [
    "react",
    "reactJS",
    "es6",
    "es7",
    "es8",
    "babel",
    "javascript",
    "typescript",
    "eslint",
    "webpack",
    "webpack 5"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "cross-env BABEL_ENV=development webpack serve --config config/webpack/webpack.config.dev.js --env development",
    "build": "cross-env BABEL_ENV=production webpack --config=config/webpack/webpack.config.prod.js --env production",
    "profile-build": "webpack --mode production --json --profile --progress > build-stats-prod.json",
    "analyse-build": "cross-env ANALYSE_BUNDLE=true BABEL_ENV=production webpack --config=config/webpack/webpack.config.prod.js --env.NODE_ENV=production --info --progress --colors --display-error-details",
    "test": "jest -c config/jest.config.js",
    "test-changed": "test -o",
    "test-watch": "test --watchAll",
    "test-update-snaps": "test -u",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-syntax-decorators": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-actions": "^6.1.2",
    "@storybook/addon-essentials": "^6.1.2",
    "@storybook/addon-links": "^6.1.2",
    "@storybook/react": "^6.1.2",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cache-loader": "^4.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.3",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^2.2.0",
    "redux-devtools-extension": "^2.13.8",
    "source-map-loader": "^1.1.2",
    "thread-loader": "^3.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "web-vitals": "^1.0.1",
    "webpack": "^5.6.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.4.0",
    "webpack-pwa-manifest": "^4.3.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
