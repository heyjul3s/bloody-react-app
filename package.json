{
  "name": "bloody-react-app",
  "version": "1.0.2",
  "author": "Julian Low <julian.cy.low@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heyjul3s/bloody-react-app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heyjul3s/bloody-react-app"
  },
  "description": "A basic Typescript React app boilerplate to help you get started sans middleware store.",
  "main": "./src/index.js",
  "keywords": [
    "react",
    "reactJS",
    "es6",
    "es7",
    "es8",
    "babel",
    "javascript",
    "typescript",
    "eslint",
    "webpack",
    "webpack 4",
    "jest",
    "docz"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "cross-env BABEL_ENV=development webpack-dev-server --config config/webpack/webpack.config.dev.js --env.NODE_ENV=development --env.PLATFORM_ENV=web --info --progress --colors --display-error-details",
    "build": "cross-env BABEL_ENV=production webpack --config=config/webpack/webpack.config.prod.js --env.NODE_ENV=production --env.PLATFORM_ENV=web --info --progress --colors --display-error-details",
    "profile-build": "webpack --mode production --json --profile --progress > build-stats-prod.json",
    "analyse-build": "cross-env ANALYSE_BUNDLE=true BABEL_ENV=production webpack --config=config/webpack/webpack.config.prod.js --env.NODE_ENV=production --env.PLATFORM_ENV=web --info --progress --colors --display-error-details",
    "test": "jest -c config/jest.config.js",
    "test-changed": "test -o",
    "test-watch": "test --watchAll",
    "test-update-snaps": "test -u",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "docz-dev": "docz dev --config config/docz.js",
    "docz-build": "docz build--config config/docz.js",
    "docz-serve": "docz build && docz serve"
  },
  "dependencies": {
    "docz": "^2.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/plugin-syntax-decorators": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@testing-library/react": "^10.4.2",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cache-loader": "^4.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.5",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "jest-cli": "^26.1.0",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "source-map-loader": "^1.0.0",
    "thread-loader": "^2.1.3",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2",
    "webpack-pwa-manifest": "^4.2.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
