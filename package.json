{
  "name": "bloody-react-app",
  "version": "1.0.1",
  "author": "Julian Low <julian.cy.low@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heyjul3s/bloody-react-app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heyjul3s/bloody-react-app"
  },
  "description": "A basic Typescript React app boilerplate to help you get started sans middleware store.",
  "main": "./src/index.js",
  "keywords": [
    "react",
    "reactJS",
    "es6",
    "es7",
    "es8",
    "babel",
    "javascript",
    "typescript",
    "webpack",
    "webpack 4",
    "jest",
    "enzyme"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify"
    }
  },
  "sideEffects": false,
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>config/enzyme.config.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(tsx|ts)?$": "ts-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(redux-persist)/)"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx|js)$"
  },
  "scripts": {
    "start": "cross-env BABEL_ENV=development webpack-dev-server --config config/webpack/webpack.config.dev.js --env.NODE_ENV=development --env.PLATFORM_ENV=web --info --progress --colors --display-error-details",
    "build": "cross-env BABEL_ENV=production webpack --config=config/webpack/webpack.config.prod.js --env.NODE_ENV=production --env.PLATFORM_ENV=web --info --progress --colors --display-error-details",
    "profile-build": "webpack --mode production --json --profile --progress > build-stats-prod.json",
    "test": "jest",
    "test-changed": "jest -o",
    "test-watch": "jest --watchAll",
    "test-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test-clear-snaps": "npm run test:watch -u",
    "storybook": "start-storybook -p 5555",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-decorators": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-jest": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/addon-storysource": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/cli": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@types/enzyme": "^3.9.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/webpack-env": "^1.13.9",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cache-loader": "^4.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "git-revision-webpack-plugin": "^3.0.3",
    "glob": "^7.1.4",
    "happypack": "^5.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-css-modules-transform": "^2.5.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-sass": "^0.3.5",
    "prettier": "^1.18.2",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "storybook-addon-jsx": "^7.1.2",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "thread-loader": "^2.1.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "tslint-webpack-plugin": "^2.1.0",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "webpack-pwa-manifest": "^4.0.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
